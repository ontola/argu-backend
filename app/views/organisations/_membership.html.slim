li[id=('list_item_'+f.object.id.to_s)]
  =image_tag f.object.user.profile.profile_photo, class: 'profile-photo'
  p= f.object.user.display_name
  =f.input :role, as: :inline_select, collection: Membership.roles.keys
  =link_to 'x', organisation_membership_path(@org, f.object), remote: true, method: :delete, data: {confirm: "_sure?_"}