<% unless current_user.nil? %><div class="votebtn" id="<%= argument.id %>"><p class="btn">
		<%= current_user.voted_on?(argument) ? 
		(link_to '-', argument_destroy_vote_path(argument), :remote => true) :
		(link_to '+', argument_create_vote_path(argument), :remote => true) %>
</p></div><% end %>